<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="300"
    android:viewportHeight="300">
  <group>
    <clip-path
        android:pathData="m0,0l300,0l0,300l-300,0l0,-300z"/>
    <path
        android:pathData="m0,0l300,0l0,300l-300,0z"
        android:fillColor="#000000"
        android:fillAlpha="0"
        android:fillType="evenOdd"
        android:strokeColor="#00000000"
        android:strokeLineCap="square"/>
    <path
        android:pathData="m-2.25,0l302.24,0l0,300l-302.24,0z"
        android:fillType="evenOdd"
        android:strokeColor="#00000000"
        android:strokeLineCap="square">
      <aapt:attr name="android:fillColor">
        <gradient
            android:startX="-2.25"
            android:startY="-0"
            android:endX="-2.25"
            android:endY="300"
            android:tileMode="clamp"
            android:type="linear">
          <item android:offset="0" android:color="#FFDBD4EB"/>
          <item android:offset="1" android:color="#FF9180BB"/>
        </gradient>
      </aapt:attr>
    </path>
  </group>
</vector>
